{% extends '::base.html.twig'%}
{% block body %}
<h1>{% trans %}Usuarios{% endtrans %}</h1>
<a class="btn" href="{{ path('wits_users_new')}}">Registrar usuario</a>
{% if (users) %}
<ul id="users">
    {% for user in users %}
    <li><a href="{{ path('wits_users_edit', {'id': user.id}) }}">{{ user.username }}</a></li>
    {% endfor %}
</ul>

{% endif %}

{% endblock %}
