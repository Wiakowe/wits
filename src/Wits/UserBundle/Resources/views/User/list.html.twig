{% extends '::base.html.twig'%}
{% block body %}
Users
<br />
<a href="{{ path('wits_user_new')}}">Registrar usuario</a>
{% if (users) %}
<ul id="users">
    {% for user in users %}
    <li><a href="{{ path('wits_user_edit', {'id': user.id}) }}">{{ user.username }}</a></li>
    {% endfor %}
</ul>

{% endif %}

{% endblock %}
