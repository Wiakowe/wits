<form class="iskoa_form" action="{{ path('wits.user.security.login_check') }}" method="post">
    {% if error %}
    <div class="error">{{ error.message|trans }}</div>
    {% endif %}
    <label for="username">{% trans %}E-mail{% endtrans %}</label>
    <input type="email" id="username" name="_username" value="{{ last_username }}" />

    <label for="password">{% trans %}ContraseÃ±a{% endtrans %}</label>
    <input type="password" id="password" name="_password" />

    <input type="submit" />
